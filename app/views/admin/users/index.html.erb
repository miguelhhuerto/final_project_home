<h1>Admin Users</h1>
<table class="table table-striped table-hover">
  <thead>
  <td>Parent E-mail</td>
  <td>Username</td>
  <td>E-mail</td>
  <td>Total Deposit</td>
  <td>Members Total Deposit</td>
  <td>Coins</td>
  <td>Total Used Coins</td>
  <td>Children Members</td>
  <td>Phone Number</td>
  </thead>

  <% @users.each do |user| %>
    <tr>
      <% if user.parent_id.present? %>
        <td> <%= User.find_by(id: user.parent_id).email%> </td>
      <% else %>
      <td> <%=""%> </td>
      <% end %>
      <td><%= link_to user.username, new_admin_user_orders_increase_path(user.id)%></td>
      <td><%= user.email %></td>
      <td></td>
      <td></td>
      <td><%= user.coins %></td>
      <td></td>
      <td><%= user.children_members %></td>
      <td><%= user.phone_number %></td>
    </tr>
  <% end %>
</table>